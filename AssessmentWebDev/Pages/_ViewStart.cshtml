@using AssessmentWebDev.Database.Items

@{
    if (User.IsInRole(UserRoles.Manager))
    {
        Layout = "Shared/_LayoutManager";
    }
    else if(User.IsInRole(UserRoles.Server))
    {
        Layout = "Shared/_LayoutServer";
    }
    else
    {
        Layout = "Shared/_LayoutAnon";
    }
    
}